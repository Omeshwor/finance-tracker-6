<table class="table table-bordered table-dark table-hover col-9">
    <thead>
        <tr>
            <th scope="col">Ticker</th>
            <th scope="col">Company Name</th>
            <th scope="col">#</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @tracked_stock.each do |stock| %>
            <tr>
                <td><%= stock.ticker %></td>
                <td><%= stock.name %></td>
                <td><%= stock.last_price %></td>
                <td><%= '#' %></td>
            </tr>
        <% end %>
    </tbody>
</table>
